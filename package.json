{
  "name": "will-d-wang",
  "version": "14.2.5",
  "private": true,
  "packageManager": "pnpm@8.12.1",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "postbuild": "pagefind --site .next/server/app --output-path public/_pagefind",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepare": "husky install",
    "eslint-fix": "eslint . --fix",
    "eslint-pre-commit": "eslint --fix",
    "mdlint-fix": "markdownlint-cli2 --fix '**/*.md' '!.trunk' '!node_modules'",
    "mdlint-pre-commit": "markdownlint-cli2 --fix",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "*.{js,mjs,cjs,jsx,ts,tsx}": "pnpm run eslint-pre-commit",
    "*.{md,mdx}": "pnpm run mdlint-pre-commit"
  },
  "dependencies": {
    "@mdx-js/loader": "^3.1.1",
    "@mdx-js/react": "^3.1.1",
    "@next/mdx": "^16.1.6",
    "next": "^15",
    "nextra": "^4.6.1",
    "nextra-theme-blog": "^4.6.1",
    "nextra-theme-docs": "^4.6.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.30",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/parser": "^8.55.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.2.5",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.3.0",
    "markdownlint-cli2": "^0.11.0",
    "pagefind": "^1.4.0",
    "prettier": "3.1.1",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.3"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=20.0"
  }
}
